{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test": {
      "dependsOn": ["^test"]
    },
    "build": {
      "dependsOn": ["lint", "^build"],
      "outputs": ["/dist/**"]
    },
    "build:tauri": {
      "dependsOn": ["^build", "^build:tauri"],
      "outputs": ["/dist/**", "/src-tauri/target/**"]
    },
    "lint": {},
    "lint:fix": {}
  }
}