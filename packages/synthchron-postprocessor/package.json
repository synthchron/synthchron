{
  "name": "@synthchron/postprocessor",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "clean": "yarn clean:dist && yarn clean:modules",
    "lint": "npx eslint src test --ext .ts --ext .tsx",
    "lint:fix": "npx eslint src test --ext .ts --ext .tsx --fix",
    "prebuild": "yarn run clean:dist",
    "build": "tsc",
    "start": "node dist/src/index",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "pretest": "yarn run build",
    "test": "mocha dist/test/**/*.js",
    "test:dev": "ts-mocha test/**/*.spec.ts -w --watch-files '**/*.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.2.3",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.7",
    "eslint": "^8.37.0",
    "@synthchron/eslint-config": "*",
    "@synthchron/types": "*",
    "mocha": "^10.1.0",
    "rimraf": "^5.0.0",
    "sinon": "^15.1.0",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "eslint-plugin-prettier": "^4.2.1"
  }
}
